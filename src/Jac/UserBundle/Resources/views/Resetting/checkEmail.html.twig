{% extends "JacUserBundle::layout.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}

{# Le titre de la page (defaut MathSup) #}
{% block title %}{{ 'resetting.flash.success '|trans }} | {{ parent() }}{% endblock %}
{# La description de la page (defaut vide) #}
{% block description %}{% endblock %}
{# Les mots clés de la page (defaut vide) #}
{% block keywords %}{% endblock %}
{# Indication pour les robots de la page  (defaut all) #}
{% block robots %}noindex, nofollow{% endblock %}

{# Le titre de l'entête du document #}
{% block entetetitle %}{{ 'resetting.flash.success '|trans }}| {{ parent() }}{% endblock %}
{# La barre latéralke de la page #}
{% block aside %}{{ parent() }}{% endblock %}
{% block titre %} {{ 'resetting.flash'|trans({}, 'FOSUserBundle') }}  <span style="font-size: 12px;float: right; color: #720e9e;">  <i class="fa fa-phone"> </i> MyContacts </span>{% endblock  %}

{% block fos_user_content %}
    <p class="alert alert-success alert-dismissible">
        <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        {{ 'resetting.check_email'|trans({'%email%': email}) }}
    </p>
{% endblock fos_user_content %}